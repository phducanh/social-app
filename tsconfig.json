{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components"
      ],
      "@public/*": [
        "public/*"
      ],
      "@public": [
        "public"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@styles": [
        "styles"
      ],
      "@utils": [
        "utils"
      ],
      "@contract": [
        "constant/ABI"
      ],
      "@contract/*": [
        "constant/ABI/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@redux": [
        "src/redux"
      ],
      "@i18n": [
        "./i18n"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/index.tsx", "pages/login/index.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
